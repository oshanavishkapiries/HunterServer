# LinkedIn Job Automator - Environment Configuration
# Copy this file to .env and fill in your values

# ============================================
# AUTHENTICATION METHOD
# ============================================
# Options: "cookie" or "credentials"
AUTH_METHOD=cookie

# ============================================
# COOKIE-BASED AUTH (when AUTH_METHOD=cookie)
# ============================================
# Path to your LinkedIn cookies JSON file
COOKIE_FILE_PATH=cookie.json

# ============================================
# CREDENTIAL-BASED AUTH (when AUTH_METHOD=credentials)
# ============================================
LINKEDIN_USERNAME=
LINKEDIN_PASSWORD=

# ============================================
# JOB SEARCH SETTINGS
# ============================================
JOB_KEYWORDS=Software Engineer
JOB_LOCATION=Remote